<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.instagram.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.instagram.ui.saerch.SearchFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_market"
        android:name="com.example.instagram.ui.market.MarketFragment"
        android:label="@string/title_market"
        tools:layout="@layout/fragment_market" />
    <fragment
        android:id="@+id/navigation_shortmovie"
        android:name="com.example.instagram.ui.shortmovie.ShortMovieFragment"
        android:label="fragment_short_movie"/>
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.instagram.ui.profile.ProfileFragment"
        android:label="fragment_profile" />
</navigation>